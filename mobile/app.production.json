{
  "expo": {
    "name": "Quantora Wallet",
    "slug": "quantora-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.quantora.wallet",
      "versionCode": 10,
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.quantora.wallet",
      "buildNumber": "10",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "extra": {
      "apiUrl": "https://api.quantora.com",
      "sentryDsn": "https://your-sentry-public-key@sentry.io/project-id",
      "googleClientId": "1234567890-abc.apps.googleusercontent.com"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "privacy": "public",
    "platforms": ["ios", "android", "web"]
  }
}